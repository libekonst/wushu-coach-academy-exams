        {
          "id": "3b761834-aced-421f-95d1-6937189ac9cb",
          "text": "Αντοχή στην κόπωση",
          "isCorrect": false
        },
        {
          "id": "222dcd46-33f5-40c1-9ced-fcea93040f4a",
          "text": "Δύναμη της κινητικής μονάδας",
          "isCorrect": false
        }
      ]
    },
    {
      "id": "a4de4eba-282b-48a6-9d39-7a0911c2bb26",
