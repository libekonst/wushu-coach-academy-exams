        },
        {
          "id": "1ed61f8b-ca05-4632-9b0c-2124cd6e1338",
          "text": "Μεγάλης διάρκειας",
          "isCorrect": false
        },
        {
          "id": "25229272-e566-416a-ae70-7f8d8e369389",
          "text": "Μικρής διάρκειας και υψηλής ισχύος",
          "isCorrect": true
        },
        {
          "id": "95332f2e-5f53-4dc2-b620-4c3b5a1aeb4e",
          "text": "Μικρής διάρκειας",
          "isCorrect": false
