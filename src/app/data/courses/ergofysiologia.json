        {
          "id": "47f253cf-6d23-4cab-80d7-8356186a9bb2",
          "text": "Δύναμη της κινητικής μονάδας",
          "isCorrect": false
        }
      ]
    },
    {
      "id": "53bf185e-fb49-41b1-97b2-58b0070421e8",
