        {
          "id": "b17efee7-c6f0-4cf3-ba5d-3fc88e1ca03a",
          "text": "Ανταγωνιστής",
          "isCorrect": false
        },
        {
          "id": "44336d9b-05b2-44aa-a3cc-298ee8189f63",
          "text": "Όλες οι απαντήσεις είναι σωστές",
          "isCorrect": true
        },
        {
          "id": "8be11d51-4443-4e91-a23c-90e397227cd4",
          "text": "Αγωνιστής",
          "isCorrect": false
        },
        {
          "id": "a622aa5a-bf08-474e-8dd6-f6fd23956413",
          "text": "Συνεργός",
          "isCorrect": false
        }
      ]
    },
    {
      "id": "0ef0f5d1-5f95-4740-8b81-c98c3ecf4b4a",
