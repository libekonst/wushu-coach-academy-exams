        },
        {
          "id": "d2c2969c-0aef-4e00-97af-3dfaf986649a",
          "text": "Μεγάλης διάρκειας",
          "isCorrect": false
        },
        {
          "id": "9f78a8b0-b36b-41d0-8f97-c3d496cb081d",
          "text": "Μικρής διάρκειας και υψηλής ισχύος",
          "isCorrect": true
        },
        {
          "id": "406d06fa-0dc6-49e9-95c5-dc3895cae557",
          "text": "Μικρής διάρκειας",
          "isCorrect": false
